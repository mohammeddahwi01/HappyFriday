<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <ordersexporttool>
            <license>
                <extension_label>Orders Export Tool</extension_label>
                <extension_version>7.0.1.5</extension_version>
                <extension_code>oet</extension_code>
                <get_online_license>1</get_online_license>
                </license>
            <settings>
                <report_title>Orders Export Tool - report </report_title>
            </settings>
            <advanced>
                <enable_log>1</enable_log>
                <grid>Magento\Sales\Block\Adminhtml\Order\Grid</grid>
            </advanced>
            <system>
                <memory_limit>0</memory_limit>
                <preview>10</preview>
                <buffer>10</buffer>
                <sqlsize>1500</sqlsize>
                <filters>10</filters>
            </system>
        </ordersexporttool>
    </default>
   
</config>
