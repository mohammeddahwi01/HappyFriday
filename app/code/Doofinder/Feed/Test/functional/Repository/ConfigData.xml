<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../vendor/magento/mtf/Magento/Mtf/Repository/etc/repository.xsd">
    <repository class="Magento\Config\Test\Repository\ConfigData">
        <dataset name="doofinder_search_1_api_key">
            <field name="doofinder_config_config/doofinder_account/api_key" xsi:type="array">
                <item name="scope" xsi:type="string">default</item>
                <item name="scope_id" xsi:type="number">0</item>
                <item name="value" xsi:type="string">DOOFINDER_ACCOUNT_API_KEY</item>
            </field>
        </dataset>
        <dataset name="doofinder_search_1_api_key_rollback">
            <field name="doofinder_config_config/doofinder_account/api_key" xsi:type="array">
                <item name="scope" xsi:type="string">default</item>
                <item name="scope_id" xsi:type="number">0</item>
                <item name="value" xsi:type="string"></item>
            </field>
        </dataset>
        <dataset name="doofinder_search_2_hash_id">
            <field name="doofinder_config_config/doofinder_search_engine/hash_id" xsi:type="array">
                <item name="scope" xsi:type="string">stores</item>
                <item name="scope_id" xsi:type="number">1</item>
                <item name="value" xsi:type="string">DOOFINDER_SEARCH_ENGINE_HASH_ID</item>
            </field>
        </dataset>
        <dataset name="doofinder_search_2_hash_id_rollback">
            <field name="doofinder_config_config/doofinder_search_engine/hash_id" xsi:type="array">
                <item name="scope" xsi:type="string">stores</item>
                <item name="scope_id" xsi:type="number">1</item>
                <item name="value" xsi:type="string"></item>
            </field>
        </dataset>
        <dataset name="doofinder_search_3_engine">
            <field name="catalog/search/engine" xsi:type="array">
                <item name="scope" xsi:type="string">default</item>
                <item name="scope_id" xsi:type="number">0</item>
                <item name="label" xsi:type="string">Doofinder</item>
                <item name="value" xsi:type="string">doofinder</item>
            </field>
        </dataset>
        <dataset name="doofinder_search_3_engine_rollback">
            <field name="catalog/search/engine" xsi:type="array">
                <item name="scope" xsi:type="string">default</item>
                <item name="scope_id" xsi:type="number">0</item>
                <item name="value" xsi:type="string">mysql</item>
            </field>
        </dataset>
    </repository>
</config>
